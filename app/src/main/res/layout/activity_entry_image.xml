<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_entry_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <Button
        android:id="@+id/entry_image_submit_button"
        style="@style/AppTheme.BottomNavigationButton"
        android:text="@string/common_submit"
        />

    <FrameLayout
        android:id="@+id/entry_image_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/entry_image_submit_button"
        android:layout_alignParentTop="true"
        />

</RelativeLayout>
